{
  "name": "webfolio",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Abubakir Shavkatov",
    "url": "https://warnigo.uz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Warnigo/Webfolio"
  },
  "scripts": {
    "dev": "node scripts/dev.mjs && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --max-warnings=0 src",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,md}",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:check": "prettier -c .",
    "prepare": "husky install",
    "types": "tsc --noEmit",
    "shadcn": "pnpm dlx shadcn@latest",
    "magic": "pnpm dlx magicui-cli",
    "aceternity": "npx aceternity-ui@latest",
    "clean": "rimraf .next && rimraf .next/cache && rimraf node_modules",
    "clean:build": "rimraf .next",
    "clean:cache": "rimraf .next/cache",
    "analyze:build": "ANALYZE=true next build",
    "json:server": "pnpm dlx json-server --watch db.json --port 5001",
    "sort:packages": "node scripts/sort-packages.mjs",
    "check:packages": "node scripts/check-package-versions.mjs",
    "check:env": "node scripts/check-env.mjs",
    "check:names": "node scripts/check-names.mjs"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --write"
    ],
    "**/*.{json,md,webmanifest}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-avatar": "1.1.0",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "@radix-ui/react-hover-card": "1.1.1",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-separator": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tooltip": "1.1.2",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/typography": "0.5.15",
    "@tanstack/react-query": "5.52.1",
    "@tanstack/react-query-devtools": "5.52.1",
    "@uidotdev/usehooks": "2.4.1",
    "chalk": "5.3.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "framer-motion": "11.3.30",
    "lucide-react": "0.435.0",
    "next": "14.2.6",
    "next-international": "1.2.4",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-intersection-observer": "9.13.0",
    "react-use": "17.5.1",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.13",
    "tailwindcss-animate": "1.0.7",
    "usehooks-ts": "3.1.0",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/node": "20.14.14",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-secrets": "1.0.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-perf": "3.3.2",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-unused-imports": "3.2.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "6.0.1",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
